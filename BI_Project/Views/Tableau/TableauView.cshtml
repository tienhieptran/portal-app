
@model BI_Core.Tableau.TableauModel


@{
    //ViewBag.Title = "EmbedTableau";
    //Layout = "~/Views/Shared/_Layout.cshtml";
    var _listDepartment = ViewBag.ListDepartment;
    var _orgCode = "MA_DVIQLY";
    var _orgCode1 = "MA_FILTER";
}
<div class="content" style="padding-bottom:5px !important">
    <div class="panel panel-default">

        @*<div class="form-group row">
                @{
                    string blockTitle = Model.pathTableau;
                }
                <h4>@blockTitle</h4>
            </div>*@
        <div class='tableauPlaceholder' style='width: 1350px; height: 660px;'>
            <div class="row">
                <object class='tableauViz' style='display:none; width:100%;height:768px'>
                    @*<param name='site_root' value='' />*@
                    @*@if (Model.Site_Root == "")
                        {
                            <param name='site_root' value='@Model.Site_Root' />
                        }*@

                    <param name='embed_code_version' value='3' />
                    <param name='site_root' value='' />
                    <param name='name' value='@Model.TableauUrl' />
                    <param name='ticket' value="@Model.Ticket" />

                    @if (Model.filter != null)
                    {
                        if (Model.filter.Length > 0)
                        {
                            <param name='filter' value='@_orgCode=@Model.filter' />
                            <param name='filter' value='@_orgCode1=@Model.filter2' />

                        }

                    }
                    else
                    {
                       
                            <param name='filter' value='@_orgCode=@_listDepartment' />
                        

                    }

                    <param name='tabs' value='no' />
                    <param name='toolbar' value='top' />
                    @*<param name='filter' value=':original_view=no' />*@
                    <param name='alerts' value='no' />
                    <param name='showShareOptions' value='no' />
                    <param name="linktarget" value="_blank" />
                    <param name='showAppBanner' value='false' />
                    <param name='subscriptions' value='no' />
                    <param name='tabs' value='no' />
                    @*<param name='filter' value='iframeSizedToWindow=no' />*@

                </object>
            </div>
        </div>
        <br />
    </div>
</div>


<script type='text/javascript' src='https://ibis.ierp.vn/javascripts/api/viz_v1.js'></script>